    "mysql": "^2.18.1",
    "mysql2": "^1.7.0",
    "n": "^6.7.0",
    "nightmare": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.22.3",
    "sequelize-auto": "^0.5.3",
    "twitter-text": "^3.1.0",
    "typeorm": "^0.2.25",
    "vo": "^4.0.2",
    "winser": "^1.0.3",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
