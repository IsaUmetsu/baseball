{
  "name": "baseball",
  "version": "1.0.0",
  "description": "",
  "main": "process.js",
  "scripts": {
    "start": "nodemon process.js",
    "dev:mac": "NODE_ENV=mac DB_ENV=mac node scheduler/player.js",
    "dev:windows": "SET NODE_ENV=windows&&SET DB_ENV=windows&&node scheduler/player.js",
    "dev:win-mac": "SET NODE_ENV=windows&&SET DB_ENV=mac&&node scheduler/player.js",
    "casper": "casperjs --verbose --log-level=debug process.web.js",
    "check:dup": "ts-node for_python/check.duplicate.json.ts",
    "import": "ts-node for_python/process.json.ts",
    "import:pitch:stats": "ts-node for_python/process.pitcher.stats.ts",
    "import:bat:stats": "ts-node for_python/process.batter.stats.ts",
    "forever:test": "ts-node for_python/scheduler.ts",
    "forever": "forever start -v -c ts-node for_python/scheduler.ts",
    "update:poc": "ts-node for_python/query/update_plus_out_count.ts",
    "gen:model": "typeorm-model-generator -h localhost -d baseball_2020 -p 3306 -u root -e mysql -o ./for_python/.",
    "gen:query": "ts-node for_python/query/createQuery.ts > ./gen_query.sql",
    "bat:rc5:team": "K=bat T=team ts-node for_python/query/recent_5days.ts",
    "bat:rc5:team:tw": "K=bat T=team ts-node for_python/query/recent_5days.ts -t",
    "bat:rc5:all": "K=bat T=all ts-node for_python/query/recent_5days.ts",
    "bat:rc5:all:tw": "K=bat T=all ts-node for_python/query/recent_5days.ts -t",
    "bat:domain:hand": "ts-node for_python/query/bat_domain_hand.ts",
    "bat:domain:hand:tw": "ts-node for_python/query/bat_domain_hand.ts -t",
    "bat:major:pos": "ts-node for_python/query/bat_major_position.ts",
    "bat:major:pos:tw": "ts-node for_python/query/bat_major_position.ts -t",
    "day:bat:team": "PR=D ts-node for_python/query/bat_team.ts",
    "dow:stand": "ts-node for_python/query/day_of_the_week_standings.ts",
    "dow:stand:tw": "ts-node for_python/query/day_of_the_week_standings.ts -t",
    "dow:bat:champ": "PR=DO ts-node for_python/query/bat_champion.ts",
    "dow:bat:champ:tw": "PR=DO ts-node for_python/query/bat_champion.ts -t",
    "dow:bat:per:team": "ts-node for_python/query/day_of_the_week_batting_per_team.ts",
    "dow:bat:per:team:w": "ts-node for_python/query/day_of_the_week_batting_per_team.ts -t",
    "month:bat:champ": "PR=M ts-node for_python/query/bat_champion.ts",
    "month:bat:champ:tw": "PR=M ts-node for_python/query/bat_champion.ts -t",
    "month:bat:team": "PR=M ts-node for_python/query/bat_team.ts",
    "month:era:s": "ts-node for_python/query/month_era_start.ts",
    "month:era:s:tw": "ts-node for_python/query/month_era_start.ts -t",
    "month:era:m": "ts-node for_python/query/month_era_middle.ts",
    "month:era:m:tw": "ts-node for_python/query/month_era_middle.ts -t",
    "month:stand": "PR=M ts-node for_python/query/team_standings.ts",
    "month:stand:tw": "PR=M ts-node for_python/query/team_standings.ts -t",
    "month:team:onbase:ave": "ts-node for_python/query/month_team_onbase_ave.ts",
    "month:team:onbase:ave:tw": "ts-node for_python/query/month_team_onbase_ave.ts -t",
    "month:team:ave": "ts-node for_python/query/month_team_average.ts",
    "month:team:ave:tw": "ts-node for_python/query/month_team_average.ts -t",
    "month:team:ave:sp": "ts-node for_python/query/month_team_average_scoring.ts",
    "month:team:era": "PR=M ts-node for_python/query/team_era.ts",
    "month:team:era:tw": "PR=M ts-node for_python/query/team_era.ts -t",
    "month:team:era:div": "PR=M ts-node for_python/query/team_era.ts -d",
    "month:team:era:div:tw": "PR=M ts-node for_python/query/team_era.ts -d -t",
    "month:bat:title": "T=B ts-node for_python/query/month_title_holder.ts",
    "month:title": "ts-node for_python/query/month_title_holder.ts",
    "ob:rc5:all": "K=ob ts-node for_python/query/recent_5days.ts",
    "ob:rc5:all:tw": "K=ob ts-node for_python/query/recent_5days.ts -t",
    "oppo:bat": "ts-node for_python/query/opponent_batting.ts",
    "oppo:bat:tw": "ts-node for_python/query/opponent_batting.ts -t",
    "oppo:era": "ts-node for_python/query/opponent_era.ts",
    "oppo:era:tw": "ts-node for_python/query/opponent_era.ts -t",
    "ops:rc5:all": "K=ops ts-node for_python/query/recent_5days.ts",
    "ops:rc5:all:tw": "K=ops ts-node for_python/query/recent_5days.ts",
    "pitch:rc10": "ts-node for_python/query/pitch_era_10days.ts",
    "pitch:rc10:tw": "ts-node for_python/query/pitch_era_10days.ts -t",
    "pitch:gf:s": "ts-node for_python/query/pitch_ground_fly_start.ts",
    "pitch:gf:s:tw": "ts-node for_python/query/pitch_ground_fly_start.ts -t",
    "pitch:ra:s": "ts-node for_python/query/pitch_ra_per_inning_start.ts",
    "pitch:ra:s:tw": "ts-node for_python/query/pitch_ra_per_inning_start.ts -t",
    "pitch:type": "ts-node for_python/query/pitch_type.ts",
    "pitch:type:tw": "ts-node for_python/query/pitch_type.ts -t",
    "pitch:strike": "ts-node for_python/query/pitch_strike_sw_ms.ts",
    "pitch:strike:tw": "ts-node for_python/query/pitch_strike_sw_ms.ts -t",
    "pitch:strike:game": "ts-node for_python/query/pitch_strike_sw_ms_game.ts",
    "pitch:strike:game:tw": "ts-node for_python/query/pitch_strike_sw_ms_game.ts -t",
    "pitch:p:out": "ts-node for_python/query/pitch_per_out.ts",
    "pitch:p:out:tw": "ts-node for_python/query/pitch_per_out.ts -t",
    "pitch:r:ave": "ts-node for_python/query/pitch_reliever_ave_team.ts",
    "pitch:r:ave:tw": "ts-node for_python/query/pitch_reliever_ave_team.ts -t",
    "team:r:ra": "ts-node for_python/query/team_runs_runs_allowed.ts",
    "team:r:ra:tw": "ts-node for_python/query/team_runs_runs_allowed.ts -t",
    "team:situ:ob": "ts-node for_python/query/team_situation_onbase.ts",
    "team:situ:sp": "ts-node for_python/query/team_situation_onbase.ts -s",
    "week:bat:champ": "PR=W ts-node for_python/query/bat_champion.ts",
    "week:bat:champ:tw": "PR=W ts-node for_python/query/bat_champion.ts -t",
    "week:bat:team": "PR=W ts-node for_python/query/bat_team.ts",
    "week:stand": "PR=W ts-node for_python/query/team_standings.ts",
    "week:stand:tw": "PR=W ts-node for_python/query/team_standings.ts -t",
    "week:team:era": "PR=W ts-node for_python/query/team_era.ts",
    "week:team:era:tw": "PR=W ts-node for_python/query/team_era.ts -t",
    "week:team:era:div": "PR=W ts-node for_python/query/team_era.ts -d",
    "week:team:era:div:tw": "PR=W ts-node for_python/query/team_era.ts -t -d",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "nodemonConfig": {
    "ignore": [
      "./data/"
    ]
  },
