    "dow:rst:tw": "ts-node for_python/query/day_of_result.ts -t",
    "team": "ts-node for_python/query/team.ts",
    "team:tw": "ts-node for_python/query/team.ts -t",
    "team:rst:era": "ts-node for_python/query/team_of_result_era.ts",
    "team:rst:era:tw": "ts-node for_python/query/team_of_result_era.ts -t",
    "week": "ts-node for_python/query/week.ts",
    "week:tw": "ts-node for_python/query/week.ts -t",
    "week:rst": "ts-node for_python/query/week_of_result.ts",
