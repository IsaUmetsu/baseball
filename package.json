    "situ:ob": "ts-node for_python/query/situation_onbase.ts",
    "situ:sp": "ts-node for_python/query/situation_onbase.ts -s",
    "irapp": "ts-node for_python/query/inning_ra_per_p.ts",
    "irapp:tw": "ts-node for_python/query/inning_ra_per_p.ts -t",
    "dh:b": "ts-node for_python/query/domain_hand_batter.ts",
    "major:pos": "ts-node for_python/query/major_pos.ts",
    "major:pos:tw": "ts-node for_python/query/major_pos.ts -t",
    "pitch:type": "ts-node for_python/query/pitch_type.ts",
    "pitch:type:tw": "ts-node for_python/query/pitch_type.ts -t",
    "pitch:strike:sw": "S=SW ts-node for_python/query/pitch_strike_sw_lk.ts",
    "pitch:strike:sw:tw": "S=SW ts-node for_python/query/pitch_strike_sw_lk.ts -t",
    "pitch:strike:ms": "S=LK ts-node for_python/query/pitch_strike_sw_lk.ts",
    "pitch:strike:ms:tw": "S=LK ts-node for_python/query/pitch_strike_sw_lk.ts -t",
    "pitch:p:out": "ts-node for_python/query/pitch_per_out.ts",
    "update:poc": "ts-node for_python/query/update_plus_out_count.ts",
    "check:dup": "ts-node for_python/check.duplicate.json.ts",
    "gen:query": "ts-node for_python/query/createQuery.ts > ./gen_query.sql",
    "test": "echo \"Error: no test specified\" && exit 1"
