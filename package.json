    "import:bat:stats": "ts-node for_python/process.batter.stats.ts",
    "gen:model": "typeorm-model-generator -h localhost -d baseball_2020 -p 3306 -u root -e mysql -o ./for_python/.",
    "month": "ts-node for_python/query/month.ts",
    "month:tw": "ts-node for_python/query/month.ts -t",
    "month:rst": "ts-node for_python/query/month_of_result.ts",
    "month:rst:tw": "ts-node for_python/query/month_of_result.ts -t",
