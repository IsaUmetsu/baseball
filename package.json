    "@types/node": "^14.6.0",
    "@types/sequelize": "^4.28.9",
    "@types/twitter": "^1.7.0",
    "@types/validator": "^13.1.0",
    "@types/yargs": "^13.0.10",
    "log4js": "^4.5.1",
    "moment": "^2.27.0",
    "node-cron": "^2.0.3",
    "nodemon": "^1.19.4",
    "ts-node": "^9.0.0",
    "twitter": "^1.7.1",
    "typeorm-model-generator": "^0.4.2"
  }
