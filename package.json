    "month:team:era:m": "ts-node for_python/query/month_team_era_middle.ts",
    "month:team:era:m:tw": "ts-node for_python/query/month_team_era_middle.ts -t",
    "rc5": "ts-node for_python/query/recent_5days.ts",
    "rc5:tw": "ts-node for_python/query/recent_5days.ts -t",
    "dow": "ts-node for_python/query/day_of_week.ts",
    "dow:tw": "ts-node for_python/query/day_of_week.ts -t",
